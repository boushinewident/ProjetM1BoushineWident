  intrahash = {de8059fb2955aa32ba1d5d2ba2e2f966},
  journal = {J Theor Biol},
  keywords = {imported},
  local-url = {file://localhost/Users/sven/Documents/Papers/2008/Langer/J%20Theor%20Biol%202008%20Langer.pdf},
  pages = {634--641},
  rating = {0},
  read = {Yes},
  timestamp = {2009-02-08T16:38:59.000+0100},
  title = {Spatial invasion of cooperation},
  uri = {papers://B7B184F3-8CE5-4C43-B61C-B7952DE67982/Paper/p1595},
  volume = 250,
